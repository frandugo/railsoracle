<p id="notice"><%= notice %></p>

<h1>Estudiantes</h1>

<table>
  <thead>
    <tr>
      <th>Nom1</th>
      <th>Nom2</th>
      <th>Ape1</th>
      <th>Ape2</th>
      <th>Idac</th>
      <th>Idcu</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @estudiantes.each do |estudiante| %>
      <tr>
        <td><%= estudiante.nom1 %></td>
        <td><%= estudiante.nom2 %></td>
        <td><%= estudiante.ape1 %></td>
        <td><%= estudiante.ape2 %></td>
        <td><%= estudiante.acudiente_id %></td>
        <td><%= estudiante.curso_id %></td>
        <td><%= link_to 'Show', estudiante %></td>
        <td><%= link_to 'Edit', edit_estudiante_path(estudiante) %></td>
        <td><%= link_to 'Destroy', estudiante, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Estudiante', new_estudiante_path %>
